@{
    ViewBag.Title = "Home Page";
}
<style>
    #c {
        min-width: 400px;
        min-height: 400px;
    }
</style>


<canvas id="c"></canvas>

@section scripts{

    <script src="~/Scripts/fabric.js"></script>
    <script>
        var canvas = new fabric.Canvas('c');

        var rect = new fabric.Rect({
            left: 100,
            top: 100,
            fill: 'red',
            width: 20,
            height: 20,
        });

        canvas.add(rect);
    </script>
}
